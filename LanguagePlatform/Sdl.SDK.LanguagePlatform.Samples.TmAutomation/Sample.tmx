      <tuv xml:lang="en-US">
        <seg>The Check Spelling Command</seg>
      </tuv>
      <tuv xml:lang="de-DE">
        <seg>Der Befehl Rechtschreibung</seg>
      </tuv>
    </tu>
    <tu creationdate="20090504T230613Z" creationid="PEGASUS\Ziad" changedate="20090630T172125Z" changeid="PEGASUS\Administrator" lastusagedate="20090630T172125Z" usagecount="3">
