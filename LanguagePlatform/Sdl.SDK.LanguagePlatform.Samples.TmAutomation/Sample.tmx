      <prop type="x-Origin">TM</prop>
      <prop type="x-StructureContext:MultipleString">x-tm-heading</prop>
      <tuv xml:lang="en-US">
        <seg>Configuring the Spelling Checker</seg>
      </tuv>
      <tuv xml:lang="de-DE">
        <seg>Die Rechtschreibprüfung konfigurieren</seg>
      </tuv>
    </tu>
    <tu creationdate="20090504T230703Z" creationid="PEGASUS\Ziad" changedate="20090630T171536Z" changeid="PEGASUS\Administrator" lastusagedate="20090630T171536Z" usagecount="5">
